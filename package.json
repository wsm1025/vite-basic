{
  "name": "vite-basic",
  "version": "0.0.2",
  "scripts": {
    "dev": "vite",
    "serve": "npm run dev",
    "build": "vite build",
    "preview": "vite preview",
    "lint:eslint": "eslint --fix --max-warnings 0 \"src/**/*.{vue,js,jsx}\" --cache --cache-location node_modules/.cache/eslint/",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,css,less,vue,html,md}\"",
    "lint:stylelint": "stylelint --fix \"src/**/*.{css,less,html,vue}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged"
  },
  "dependencies": {
    "@vueuse/core": "^8.1.1",
    "ant-design-vue": "3.2.11",
    "axios": "^0.26.0",
    "dayjs": "^1.10.7",
    "pinia": "^2.0.11",
    "vue": "^3.2.31",
    "vue-router": "^4.0.13",
    "lodash-es": "4.17.21"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/node": "^17.0.21",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vitejs/plugin-vue-jsx": "^1.3.5",
    "@vue/compiler-sfc": "^3.2.31",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.3.0",
    "less": "^4.1.2",
    "lint-staged": "^12.3.4",
    "postcss": "^8.4.5",
    "postcss-html": "^1.3.0",
    "postcss-less": "^6.0.0",
    "prettier": "2.5.1",
    "rimraf": "^3.0.2",
    "stylelint": "^14.5.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "unplugin-vue-components": "^0.17.21",
    "vite": "^2.7.2",
    "vite-plugin-compression": "^0.5.1",
    "unplugin-auto-import": "^0.9.3"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{less,scss,styl,html}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "description": "Node.js v14.0.0+",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wsm1025/vite-basic.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wsm1025/vite-basic/issues"
  },
  "homepage": "https://github.com/wsm1025/vite-basic#readme"
}
